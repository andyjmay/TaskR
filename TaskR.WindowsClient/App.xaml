<Application x:Class="TaskR.WindowsClient.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="clr-namespace:TaskR.WindowsClient.ViewModels"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <vm:ViewModelLocator x:Key="Locator" />
            <Color x:Key="AccentColor">#FF0E65DA</Color>
            <Color x:Key="BackgroundColor">#FF272727</Color>
            <Color x:Key="BackgroundColorLight">White</Color>
        </ResourceDictionary>        
    </Application.Resources>
</Application>
